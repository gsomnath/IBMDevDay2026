# ===========================================
# IBM Dev Day 2026 Frontend - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values
# NEVER commit the .env file to version control!

# Demo Login Credentials
REACT_APP_DEMO_USER=demo
REACT_APP_DEMO_PASSWORD=your_password_here

# Usage Limits
REACT_APP_MAX_CALLS_PER_DAY=200

# WatsonX Configuration
WATSONX_PROJECT_ID=your_project_id_here
WATSONX_URL=https://us-south.ml.cloud.ibm.com

# Watson Orchestrate Configuration
REACT_APP_WXO_ORCHESTRATION_ID=your_orchestration_id_here
REACT_APP_WXO_HOST_URL=https://eu-gb.watson-orchestrate.cloud.ibm.com
REACT_APP_WXO_CRN=crn:v1:bluemix:public:watsonx-orchestrate:eu-gb:a/your_account:your_instance::

# Email Rewriter Agent
REACT_APP_EMAIL_AGENT_ID=your_email_agent_id_here
REACT_APP_EMAIL_AGENT_ENV_ID=your_email_agent_env_id_here

# BAU Estimate Agent
REACT_APP_BAU_AGENT_ID=your_bau_agent_id_here
REACT_APP_BAU_AGENT_ENV_ID=your_bau_agent_env_id_here

# Server Port (for production)
PORT=8080
